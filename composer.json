{
	"name": "fliglio/health",
	"description": "Fliglio Health",
	"type": "library",
	"license": "GPL-3.0+",
	"authors": [{
		"name": "Philip Whitt",
		"email": "philipwhitt88@gmail.com",
		"homepage": "http://www.philipwhitt.com"
	}],
	"require": {
		"php"                     : "^7.4||^8",
		"psr/log"                 : "1.*"
	},
	"require-dev": {
		"fliglio/http"            : "2.1.*",
		"php-amqplib/php-amqplib" : "3.*",
		"mockery/mockery"         : "1.*",
		"phpunit/phpunit"         : "^8"
	},
	"autoload": {
		"psr-4": {
			"Fliglio\\Health\\" : "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Fliglio\\Health\\" : "test"
		}
	},
	"suggest": {
		"php-amqplib/php-amqplib": "For RabbitMQ health checks",
		"ext-pdo": "For mysql health checks"
	}
}
